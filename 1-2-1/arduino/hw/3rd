void setup() {
  // put your setup code here, to run once:
  pinMode(2, OUTPUT);
}
//202135574 Jeon sihyeon
void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(2, HIGH);
  delay(1000);
  digitalWrite(2, LOW);
  delay(1000);
}


2.

void setup() {
  // put your setup code here, to run once:
  pinMode(12, OUTPUT);
  pinMode(13, OUTPUT);
}
//202135574 Jeon sihyeon
void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(12, HIGH);
  delay(100);
  digitalWrite(12, LOW);
  delay(100);
  digitalWrite(13, HIGH);
  delay(100);
  digitalWrite(13, LOW);
  delay(100);
}


3.

void setup() {
  // put your setup code here, to run once:
  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
}
//202135574 Jeon sihyeon
void loop() {
  // put your main code here, to run repeatedly:
  for(int i = 0; i<256; i++){
    analogWrite(10,i);
    analogWrite(11,255-i);
    }
  for(int i = 0; i<256; i++){
    analogWrite(11,i);
    analogWrite(10,255-i);
    }
}


